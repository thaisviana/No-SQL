<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    
    <link href="@Url.Content("~/Static/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Static/css/rastreabilidade.css")" rel="stylesheet" type="text/css" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js" type="text/javascript"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.23/jquery-ui.min.js"
        type="text/javascript"></script>
    <script src="@Url.Content("~/Static/js/bootbox.js")" type="text/javascript" />
    <script src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.10.0/jquery.validate.min.js"
        type="text/javascript"></script>
    <script src="@Url.Content("~/Static/js/mustache.js")" type="text/javascript" />
    <script type="text/javascript">
        document.write('<script src="@Url.Content("~/Static/js/bootstrap.min.js")">\x3C/script>');
    </script>
    <script type="text/javascript">
        window.jQuery || document.write('<script src="@Url.Content("~/Static/js/jquery-1.8.2.min.js")">\x3C/script>');
    </script>
    <script type="text/javascript">
        window.jQuery.ui || document.write('<script src="@Url.Content("~/Static/js/jquery-ui-1.8.24.min.js")">\x3C/script>');
    </script>
    <link href="@Url.Content("~/Static/css/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Static/css/bootstrap-theme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>

    <script language="JavaScript" type="text/javascript" src="@Url.Content("~/Static/js/bic.js")"></script>
    
</head>
<body>
    <div class="page">
        <div id="header">
        <br />
            <div  id="title">
                <h1>Base de Conhecimento Integrada</h1>
            </div>

             <div class="col-sm-3 col-md-3 pull-right">
                <form  action="@Url.Action("Search", "Home")", method="post" class="navbar-form">
                    <div class="input-group">
                        <input size="50" type="text" class="form-control" placeholder="Search" name="srch-term" id="srch-term" />
                        <div class="input-group-btn">
                           <!-- <button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>-->
                            <button class="btn btn-default" type="submit"><img width="17" src="/Static/img/search.png"/></button>
                        </div>
                    </div>
                </form>
            </div>
        
             <br /><br />
            <div id="menucontainer" >
                <ul id="menu">
                    <li>@Html.ActionLink("Home", "Get", "Home")</li>
                    <!-- <li>@Html.ActionLink("Item de Conhecimento", "Get", "Item")</li>-->
                     
                </ul>
            </div>
        </div>

        <div id="main" style="height : 100%">

            @RenderBody()

        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
