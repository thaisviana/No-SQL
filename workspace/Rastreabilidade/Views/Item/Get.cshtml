@model BIC.Models.ItemConhecimentoModelView
@{
    ViewBag.Title = "Get";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("HandleForm", "Item", FormMethod.Post)) { 
<div class="container-fluid span10" id="main" style="background-color: white;">
	<div>&nbsp</div>
    <div class="row-fluid" >
		<label class="span2">Titulo : </label>
		<div class="span4">@Html.TextBoxFor(model => model.Nome)</div>
	</div>
    <div class="row-fluid">
		<div class="span2">Fonte :</div>
		<div class="span8">@Html.TextBoxFor(model => model.Fonte)</div>
    </div>
    <!--<div class="row-fluid">
		<div class="span2">Autores :</div>
		<div class="span8">@Html.TextBoxFor(model => model.Autores)</div>
    </div>-->
    <div class="row-fluid">
		<div class="span2">Pergunta A :</div>
		<div class="span8">@Html.TextBoxFor(model => model.PerguntaA)</div>
    </div>
    <div class="row-fluid">
		<div class="span2">Pergunta B :</div>
		<div class="span8">@Html.TextBoxFor(model => model.PerguntaB)</div>
    </div>
    <div class="row-fluid">
		<div class="span2">Pergunta C :</div>
		<div class="span8">@Html.TextBoxFor(model => model.PerguntaC)</div>
    </div>
    <div class="row-fluid">
		<div class="span2">Pergunta D :</div>
		<div class="span8">@Html.TextBoxFor(model => model.PerguntaD)</div>
    </div>

    <div class="row-fluid">
		<input id="btnSubmit" type="submit" value="Submit" />
    </div>

    
</div>
}